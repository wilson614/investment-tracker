[
  {
    "id": "deploy",
    "execute-command": "/bin/bash",
    "pass-arguments-to-command": [
      {
        "source": "string",
        "name": "/scripts/deploy.sh"
      }
    ],
    "command-working-directory": "/app",
    "include-command-output-in-response": true,
    "trigger-rule": {
      "match": {
        "type": "value",
        "value": "$DEPLOY_TOKEN",
        "parameter": {
          "source": "header",
          "name": "X-Webhook-Token"
        }
      }
    }
  }
]